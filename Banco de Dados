CREATE DATABASE tania;

CREATE TABLE usabilidade(
	nome VARCHAR (200),
	cpf VARCHAR(11) PRIMARY KEY, 
	rg VARCHAR(15),
	orgao VARCHAR(30),
	profissao VARCHAR(100),
	titulo VARCHAR(15),
	estado_civil INTEGER,
	nascimento DATE,
	sexo VARCHAR(2),
	rua_cont VARCHAR(300),
	num_cont INTEGER,
	bairro_cont VARCHAR(50),
	cidade_cont VARCHAR(50),
	uf_cont VARCHAR(3),
	cep_cont INTEGER,
	telefone_cont VARCHAR(15),
	celular VARCHAR(15),
	email VARCHAR(100),
	escolaridade INTEGER,
	rendimento NUMERIC(10,2),
	membros INTEGER,
	fantasia VARCHAR(200),
	razao VARCHAR(200),
	cnpj VARCHAR(20),
	cod_acesso VARCHAR(30),
	recibo VARCHAR(30),
	rua_emp VARCHAR(200),
	num_emp INTEGER,
	bairro_emp VARCHAR(50),
	cidade_emp VARCHAR(50),
	uf_empresa VARCHAR(3),
	cep_empresa INTEGER,
	tel_empresa VARCHAR(13),
	email_emp VARCHAR(100),
	gera INTEGER,
	valor NUMERIC(10,2),
	segmento INTEGER,
	data_abertura DATE,
	funcionarios INTEGER,
	demanda VARCHAR(5000)
	
	);
